{
  "name": "web",
  "version": "0.13.1",
  "type": "module",
  "scripts": {
    "dev:mock": "vite --open --mode mock",
    "dev": "vite --mode development",
    "dev:linux": "vite --mode development",
    "build:test": "vite build --mode test",
    "build": "vue-tsc --noEmit && vite build --mode release",
    "build:type": "vue-tsc --noEmit",
    "build:site": "vue-tsc --noEmit && vite build --mode site",
    "preview": "vite preview",
    "lint": "eslint --ext .vue,.js,.jsx,.ts,.tsx ./ --max-warnings 0",
    "lint:fix": "eslint --ext .vue,.js,jsx,.ts,.tsx ./ --max-warnings 0 --fix",
    "stylelint": "stylelint src/**/*.{html,vue,css,sass,less}",
    "stylelint:fix": "stylelint --fix src/**/*.{html,vue,css,sass,less}",
    "prepare": "node -e \"if(require('fs').existsSync('.git')){process.exit(1)}\" || is-ci || husky install",
    "site:preview": "npm run build && cp -r dist _site",
    "test": "echo \"no test specified,work in process\"",
    "test:coverage": "echo \"no test:coverage specified,work in process\""
  },
  "dependencies": {
    "@vueuse/core": "^14.0.0",
    "axios": "^1.12.2",
    "dayjs": "^1.11.18",
    "echarts": "6.0.0",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "qrcode.vue": "^3.6.0",
    "qs": "^6.14.0",
    "tdesign-icons-vue-next": "^0.4.1",
    "tdesign-vue-next": "^1.17.1",
    "tvision-color": "^1.6.0",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.12",
    "vue-router": "~4.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/echarts": "^5.0.0",
    "@types/lodash": "^4.17.20",
    "@types/mockjs": "^1.0.10",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.14.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/compiler-sfc": "~3.5.22",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue-scoped-css": "^2.12.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "less": "^4.4.2",
    "lint-staged": "^16.2.6",
    "mockjs": "^1.1.0",
    "postcss-html": "^1.8.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.6.2",
    "rspack-resolver": "^1.3.0",
    "stylelint": "~16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-order": "~7.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^3.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write",
      "npm run lint:fix"
    ],
    "*.{html,vue,css,sass,less}": [
      "npm run stylelint:fix"
    ]
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "description": "Base on tdesign-starter-cli"
}
